{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Visitor": {
    "MaxAge": "00:00:30",
    "CookieKey": "VisitorId"
  },
  "RateLimiter": {
    "MaxRequests": 3,
    "Interval": "00:01:00",
    "RedisKey": "rate"
  },
  "RedisCache": {
    "ConnectionString": "localhost",
    "Ttl": "00:00:05"
  },
  "AllowedHosts": "*"
}
